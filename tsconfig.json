{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
     "jsx": "react",
     "moduleResolution": "node",
     "target": "ESNext" ,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
  ]
}